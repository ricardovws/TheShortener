<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <runtime>
    <AppContextSwitchOverrides value="Switch.System.Net.DontEnableSchUseStrongCrypto=false" />
  </runtime>
    <appSettings>
      <add key="BitlyApiUrl" value="https://api-ssl.bitly.com/shorten?access_token={0}&amp;longUrl={1}"/>
      <add key="BitlyApiTokenFileName" value="apiToken.json"/>
    </appSettings>

</configuration>